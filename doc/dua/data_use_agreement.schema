{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "DuaList",
    "description": "List of Data Use Agreement",
    "type": "array",
    "uniqueItems": true,
    "items": {
        "type": "object",
        "required": ["id","name","path"],
        "properties": {
            "id": {
                 "description": "Unique identifier",
                 "type": "string"
             },
            "name": {
                 "description": "Full name of the Data Use Agreement",
                 "type": "string"
             },
            "path": {
                 "description": "Content of the Data Use Agreement",
                 "type": "string"
             }
        }
    }
}
